
cd ~/Desktop/ultimate

for file in ultimate*.png; do
  name="ultimate14"
  mkdir -p "../ColeccioDragonBall/Assets.xcassets/ultimate14.imageset"
  cp "ultimate14.png" "../ColeccioDragonBall/Assets.xcassets/ultimate14.imageset/ultimate14.png"
  cat > "../ColeccioDragonBall/Assets.xcassets/ultimate14.imageset/Contents.json" <<EOF
{
  "images": [
    {
      "idiom": "universal",
      "filename": "ultimate14.png",
      "scale": "1x"
    }
  ],
  "info": {
    "version": 1,
    "author": "xcode"
  }
}
