
cd ~/Desktop/ultimate

for file in ultimate*.png; do
  name="ultimate23"
  mkdir -p "../ColeccioDragonBall/Assets.xcassets/ultimate23.imageset"
  cp "ultimate23.png" "../ColeccioDragonBall/Assets.xcassets/ultimate23.imageset/ultimate23.png"
  cat > "../ColeccioDragonBall/Assets.xcassets/ultimate23.imageset/Contents.json" <<EOF
{
  "images": [
    {
      "idiom": "universal",
      "filename": "ultimate23.png",
      "scale": "1x"
    }
  ],
  "info": {
    "version": 1,
    "author": "xcode"
  }
}
