
cd ~/Desktop/ultimate

for file in ultimate*.png; do
  name="ultimate10"
  mkdir -p "../ColeccioDragonBall/Assets.xcassets/ultimate10.imageset"
  cp "ultimate10.png" "../ColeccioDragonBall/Assets.xcassets/ultimate10.imageset/ultimate10.png"
  cat > "../ColeccioDragonBall/Assets.xcassets/ultimate10.imageset/Contents.json" <<EOF
{
  "images": [
    {
      "idiom": "universal",
      "filename": "ultimate10.png",
      "scale": "1x"
    }
  ],
  "info": {
    "version": 1,
    "author": "xcode"
  }
}
