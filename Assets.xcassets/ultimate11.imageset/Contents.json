
cd ~/Desktop/ultimate

for file in ultimate*.png; do
  name="ultimate11"
  mkdir -p "../ColeccioDragonBall/Assets.xcassets/ultimate11.imageset"
  cp "ultimate11.png" "../ColeccioDragonBall/Assets.xcassets/ultimate11.imageset/ultimate11.png"
  cat > "../ColeccioDragonBall/Assets.xcassets/ultimate11.imageset/Contents.json" <<EOF
{
  "images": [
    {
      "idiom": "universal",
      "filename": "ultimate11.png",
      "scale": "1x"
    }
  ],
  "info": {
    "version": 1,
    "author": "xcode"
  }
}
