
cd ~/Desktop/ultimate

for file in ultimate*.png; do
  name="ultimate33"
  mkdir -p "../ColeccioDragonBall/Assets.xcassets/ultimate33.imageset"
  cp "ultimate33.png" "../ColeccioDragonBall/Assets.xcassets/ultimate33.imageset/ultimate33.png"
  cat > "../ColeccioDragonBall/Assets.xcassets/ultimate33.imageset/Contents.json" <<EOF
{
  "images": [
    {
      "idiom": "universal",
      "filename": "ultimate33.png",
      "scale": "1x"
    }
  ],
  "info": {
    "version": 1,
    "author": "xcode"
  }
}
