
cd ~/Desktop/ultimate

for file in ultimate*.png; do
  name="ultimate9"
  mkdir -p "../ColeccioDragonBall/Assets.xcassets/ultimate9.imageset"
  cp "ultimate9.png" "../ColeccioDragonBall/Assets.xcassets/ultimate9.imageset/ultimate9.png"
  cat > "../ColeccioDragonBall/Assets.xcassets/ultimate9.imageset/Contents.json" <<EOF
{
  "images": [
    {
      "idiom": "universal",
      "filename": "ultimate9.png",
      "scale": "1x"
    }
  ],
  "info": {
    "version": 1,
    "author": "xcode"
  }
}
